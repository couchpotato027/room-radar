{
  "buildCommand": "cd room-radar/frontend && npm install && npm run build",
  "outputDirectory": "room-radar/frontend/build",
  "devCommand": "cd room-radar/frontend && npm start",
  "installCommand": "cd room-radar/frontend && npm install",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

